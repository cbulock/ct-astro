---
import Layout from "@layouts/Layout.astro";
import EntryList from "@components/EntryList.astro";
import { entryYears, getEntriesPerYear } from "@content/entryData";

export function getStaticPaths() {
  return entryYears.map((year) => ({
    params: { year },
  }));
}

const { year } = Astro.params;

const entriesPerYear = getEntriesPerYear(year);
---

<Layout><EntryList entries={entriesPerYear} /></Layout>
